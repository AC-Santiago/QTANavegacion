<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PrimeraPagina">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF">

        <ImageView
            android:id="@+id/imageViewPrincipio"
            android:layout_width="wrap_content"
            android:layout_height="413dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="86dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="36dp"
            android:background="#00FFFFFF"
            android:contentDescription="@string/descripcion_imagen"
            app:layout_constraintBottom_toTopOf="@+id/buttoninical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/Logo" />

        <Button
            android:id="@+id/buttoninical"
            style="@style/EstiloBotonPrincipal"
            android:layout_width="143dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="134dp"
            android:layout_marginEnd="134dp"
            android:layout_marginBottom="148dp"
            android:fontFamily="@font/roboto"
            android:text="@string/iniciar"
            android:textAllCaps="false"
            android:textColor="#1B2E35"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageViewPrincipio" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>